{
  "manifest_version": 2,
  "name": "Tab & History Manager",
  "version": "0.2",
  "author": "Vitaliy Ponomarev",
  "homepage_url": "https://github.com/vponomarev/firefox-tab-manager",
  "description": "Manage tabs and history",
  "permissions": ["tabs", "history"],
  "web_accessible_resources": [
    "list.html",
    "list.js",
    "history.html",
    "history.js"
  ],
  "data_collection_permissions": { "required":  ["none"] },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Tabs & History Manager"
  },
  "icons": {
    "48": "icon.svg"
  }
}
